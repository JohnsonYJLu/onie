+
+    sprintf(cmd, "onie-syseeprom -g 0x21");
+    fpRead = popen(cmd, "r");
+    if(fpRead != NULL)
+    {
+        if(fgets(buf, sizeof(buf), fpRead) != NULL)
+        {
+            strcpy(r_str, buf);
+            if(strlen(r_str) > 3 && strstr(r_str, "ERROR") == NULL)
+            {
+                rv = 0;
+            }
+        }
+    }
+    pclose(fpRead);
+    return rv;
+}
