+int dni_product_index_get(char *productName)
+{
+    int rv = -1;
+    int check_num = 0;
+    int i = 0;
+
+    for(i = 0; i < sizeof(product)/sizeof(product[0]); i++)
+    {
+        if(strstr(productName, product[i].name) != NULL)
+        {
+            rv = i;
+            goto END;
+        }
+        else
+        {
+            check_num++;
+        }
+
+        if(check_num == sizeof(product)/sizeof(product[0]))
+        {
+            goto END;
+        }
+    }
+END:
+    return rv;
+}
